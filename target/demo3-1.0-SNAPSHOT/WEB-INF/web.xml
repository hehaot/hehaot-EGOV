<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
<!--    反馈核准件信息-->
    <servlet>
        <servlet-name>feedbackServlet</servlet-name>
        <servlet-class>auth.feedbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>feedbackServlet</servlet-name>
        <url-pattern>/feedback</url-pattern>
    </servlet-mapping>
    <!--查看核准件明细-->
    <servlet>
        <servlet-name>viewAuthServlet</servlet-name>
        <servlet-class>auth.viewAuthServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewAuthServlet</servlet-name>
        <url-pattern>/viewAuth</url-pattern>
    </servlet-mapping>
    <!--保存核准件-->
    <servlet>
        <servlet-name>saveAuthServlet</servlet-name>
        <servlet-class>auth.saveAuthServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>saveAuthServlet</servlet-name>
        <url-pattern>/saveAuth</url-pattern>
    </servlet-mapping>
    <!--查看企业明细-->
    <servlet>
        <servlet-name>ViewEnServlet</servlet-name>
        <servlet-class>fe.servlet.ViewEnServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewEnServlet</servlet-name>
        <url-pattern>/viewEn</url-pattern>
    </servlet-mapping>
<!--    动态查询企业信息-->
    <servlet>
        <servlet-name>PageQueryEnServlet</servlet-name>
        <servlet-class>fe.servlet.PageQueryEnServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PageQueryEnServlet</servlet-name>
        <url-pattern>/pageQueryEn</url-pattern>
    </servlet-mapping>
    <!--保存企业信息以及企业和投资人的关系信息-->
    <servlet>
        <servlet-name>SaveEnServlet</servlet-name>
        <servlet-class>fe.servlet.SaveEnServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SaveEnServlet</servlet-name>
        <url-pattern>/saveEn</url-pattern>
    </servlet-mapping>
    <!--验证企业的组织机构代码是否存在-->
    <servlet>
        <servlet-name>checkOrgcodeServlet</servlet-name>
        <servlet-class>fe.servlet.checkOrgcodeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>checkOrgcodeServlet</servlet-name>
        <url-pattern>/checkOrgcode</url-pattern>
    </servlet-mapping>
<!--    投资人详细信息-->
    <servlet>
        <servlet-name>ViewInvServlet</servlet-name>
        <servlet-class>com.example.demo3.ViewInvServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewInvServlet</servlet-name>
        <url-pattern>/ViewInv</url-pattern>
    </servlet-mapping>
<!--    动态参数查询-->
    <servlet>
        <servlet-name>PageQueryInvServlet</servlet-name>
        <servlet-class>com.example.demo3.PageQueryInvServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PageQueryInvServlet</servlet-name>
        <url-pattern>/pageQueryInv</url-pattern>
    </servlet-mapping>
<!--   投资人信息-->
    <servlet>
        <servlet-name>SaveInvServlet</servlet-name>
        <servlet-class>com.example.demo3.SaveInvServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SaveInvServlet</servlet-name>
        <url-pattern>/saveInv</url-pattern>
    </servlet-mapping>
<!--    退出用户登录-->
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.example.demo3.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
<!--    用户登录-->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.example.demo3.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
<!--    查询usercode是否存在-->
    <servlet>
        <servlet-name>CheckUsercodeServlet</servlet-name>
        <servlet-class>com.example.demo3.CheckUsercodeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckUsercodeServlet</servlet-name>
        <url-pattern>/checkUsercode</url-pattern>
    </servlet-mapping>
<!--    删除用户信息-->
    <servlet>
        <servlet-name>DeleteUserServlet</servlet-name>
        <servlet-class>com.example.demo3.DeleteUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteUserServlet</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <!--更新用户信息-->
    <servlet>
        <servlet-name>updateUserServlet</servlet-name>
        <servlet-class>com.example.demo3.updateUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateUserServlet</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <!--跳转到修改页面-->
    <servlet>
        <servlet-name>goUpdateServlet</servlet-name>
        <servlet-class>com.example.demo3.goUpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>goUpdateServlet</servlet-name>
        <url-pattern>/goUpdate</url-pattern>
    </servlet-mapping>
<!--    物理分页查询用户信息-->
    <servlet>
        <servlet-name>PageQueryUserServlet</servlet-name>
        <servlet-class>com.example.demo3.PageQueryUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PageQueryUserServlet</servlet-name>
        <url-pattern>/PageQueryUser</url-pattern>
    </servlet-mapping>
    <!--    保存用户信息-->
    <servlet>
        <servlet-name>InsertUserServlet</servlet-name>
        <servlet-class>com.example.demo3.InsertUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertUserServlet</servlet-name>
        <url-pattern>/saveUser</url-pattern>
    </servlet-mapping>
<!--    监控器-->
    <listener>
        <listener-class>listener.OneListener</listener-class>
    </listener>

    <!--    欢迎文件-->
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
<!--字符编码过滤器-->
    <filter>
        <filter-name>EncodingFilter</filter-name>
        <filter-class>Fillter.EncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>EncodingFilter</filter-name>
        <url-pattern>/myWeb/*</url-pattern>
    </filter-mapping>
    <!--验证是否登陆的过滤器-->
    <filter>
        <filter-name>CheckLoginFilter</filter-name>
        <filter-class>Fillter.CheckLoginFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CheckLoginFilter</filter-name>
        <url-pattern>/myWeb/*</url-pattern>
    </filter-mapping>
</web-app>